   "source": [
    "num_list = [3, 4, 5, 2, 1]\n",
    "c_num = 1\n",
    "for i in num_list:\n",
    "    c_num *= i\n",
    "print(c_num)\n",
    "\n",
    "if c_num < (sum(num_list)*sum(num_list)):\n",
    "    print(1)\n",
    "else:\n",
    "    print(0)\n"
   ]
